{
    "name": "laravelayers/laravelayers-docs",
    "description": "The Laravelayers documentation.",
    "keywords": ["framework", "laravel", "laravelayers", "documentation"],
    "version": "3.0.0",
    "license": "MIT",
    "homepage": "https://github.com/laravelayers/laravelayers",
    "support": {
        "issues": "https://github.com/laravelayers/laravelayers-docs/issues",
        "source": "https://github.com/laravelayers/laravelayers-docs"
    },
    "authors": [
        {
            "name": "Grigory Kovalev",
            "email": "grigorykov@gmail.com"
        }
    ],
    "require": {
        "laravelayers/laravelayers": "^3.0"
    },
    "autoload": {
        "psr-4": {
